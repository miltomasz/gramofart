<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="rectangle" >
            <solid android:color="#00000000" />
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="0dp"
        android:top="1dp">
        <shape android:shape="rectangle" >
            <corners android:radius="3dp" />

            <stroke
                android:width="2dp"
                android:color="#323235" />

            <solid android:color="#00000000" />
        </shape>
    </item>

</layer-list>